class Solution {
public:
    int removeElement(vector<int>& nums, int val) {
        vector<int>nums1;
        for(int i=0;i<nums.size();i++){
            if(nums[i]==val){
              continue;
            }
            nums1.push_back(nums[i]);
        }
        nums=nums1;
        return nums.size();
        
    }
};
