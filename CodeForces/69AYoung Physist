#include <bits/stdc++.h>
using namespace std;

int main() {
    int n;
    cin>>n;
    int a[n][3];
    for(int i=0;i<n;i++){
        for(int j=0;j<3;j++){
            cin>>a[i][j];
        }
    }
    int xSum=0;
    int ySum=0;
    int zSum=0;
    for(int i=0;i<n;i++){
        xSum+=a[i][0];
        ySum+=a[i][1];
        zSum+=a[i][2];
    }
    if(xSum==0 && ySum==0 && zSum==0){
        cout<<"YES"<<endl;
    }
    else{
        cout<<"NO"<<endl;
    }
    

    return 0;
}
