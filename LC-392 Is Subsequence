class Solution {
public:
    bool isSubsequence(string s, string t) {
        int k=0;
        for(int i=0;i<t.length();i++){
                if(k<s.length()&& s[k]==t[i]){
                    k++;
                }
        }
        if(k==s.length())
        return true;
        else
        return false;
    }
};
